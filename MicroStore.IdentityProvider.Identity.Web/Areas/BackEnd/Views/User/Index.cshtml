@model UserListModel
@{
    ViewData["Title"] = "Users / Backend";
    UIMenuNavigationManager.SetCurrentMenu(IdentityMenuStandard.UserMenu.Index);
}
<div class="content-wrapper">
    <div class="content-header">
        <div class="container-fluid">
            <abp-row class="mb-2">
                <abp-column size-sm=_6>
                    <h1 class="m-0">Users</h1>
                </abp-column>
                <abp-column>
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">
                            <a asp-action="Create" class="btn btn-info">
                                <i class="fas fa-plus-square mr-1"></i>
                                Create User
                            </a>
                        </li>
                    </ol>
                </abp-column>
            </abp-row>
        </div>
    </div>
    <section class="content">
        <div class="container-fluid">
            <partial name="Grids/_Grid.Users" model="Model" />
        </div>
    </section>
</div>

@section Scripts{
    <script src="~/backend/js/user/index.js"></script>
}