{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/baskets/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7268
        }
      ],

      "UpstreamPathTemplate": "/mvcgateway/baskets/{userId}",
      "UpstreamHttpMethod": [ "GET" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": [ "mvcgateway.basket.read" ]
      },

      "DelegatingHandlers": [
        "BasketTokenExchangeDelegatingHandler"
      ]
    },


