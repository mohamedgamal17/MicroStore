@model MicroStore.IdentityProvider.IdentityServer.Web.Areas.BackEnd.Models.ApiResources.ApiResourceSecretListUIModel
<abp-card>
    <abp-card-header>
        <abp-card-title class="text-lg"><i class="fas fa-info mr-1 text-lightblue"></i>Api Secrets</p></abp-card-title>
         <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus text-lg"></i></button>
        </div>
    </abp-card-header>
    <abp-card-body>
        <abp-table id="ApiResourceSecrets" striped-rows="true"></abp-table>
    </abp-card-body>
    <abp-card-footer>
        <abp-button button-type="Info" id="CreateApiSecretButton">Add New Api Secret</abp-button>
    </abp-card-footer>
</abp-card>
