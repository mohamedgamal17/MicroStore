@model MicroStore.Client.PublicWeb.Areas.Administration.Models.Inventory.InventoryItemListModel

<div class="card card-default">
    <div class="card-body">

        <table  id="invetoryitems-grid" class="table  table-hover table-striped  dataTable no-footer" width="100%">
            <thead>

                <tr>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().Thumbnail)</th>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().Name)</th>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().Sku)</th>         
                    <th>@Html.DisplayNameFor(x=> x.Data.First().Stock)</th>
                    <th></th>
                </tr>
            </thead>
        </table>

    </div>
</div>