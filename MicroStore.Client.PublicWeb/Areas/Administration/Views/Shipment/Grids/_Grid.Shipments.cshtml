@model MicroStore.Client.PublicWeb.Areas.Administration.Models.Shipments.ShipmentListModel

<div class="card card-default">
    <div class="card-body">

        <table id="shipments-grid" class="table  table-hover table-striped  dataTable no-footer" width="100%">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().OrderId)</th>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().OrderNumber)</th>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().UserId)</th>
                    <th>@Html.DisplayNameFor(x=> x.Data.First().Status)</th>
                    <th></th>
                </tr>
            </thead>

        </table>

    </div>
</div>